package UserDefinedMethods;

public class MethodExamples {
	public void srini()
	{
		System.out.println("this is a Srini User DSefine Method");
	}
	
	public static void main(String[]args)
	{
		System.out.println("this is the main Method of the java language");
		MethodExample1 m1=new MethodExample1();
		System.out.println();
		m1.srinu();
		m1.srinu();
		m1.srinu();
		
	}

}
